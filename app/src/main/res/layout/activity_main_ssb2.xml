<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivitySSB2">


    <EditText
        android:id="@+id/idEditTextSSB2"
        android:layout_width="300dp"
        android:layout_height="75dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:autofillHints="@string/idHintSSB2"
        android:hint="@string/idHintSSB2"
        android:inputType="text"
        android:selectAllOnFocus="false"
        android:singleLine="true"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </EditText>

    <TextView
        android:id="@+id/idScoreSSB2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/idScoreSSB2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idEditTextSSB2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/idPeach"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:text="@string/idPeach"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idEditTextSSB2"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idBowser"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idBowser"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idPeach"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idIceClimbers"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idIceClimbers"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idBowser"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idSheik"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idSheik"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idIceClimbers"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idZelda"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idZelda"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idSheik"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idDrMario"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idDrMario"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idZelda"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idPichu"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginStart="55dp"
        android:gravity="center"
        android:text="@string/idPichu"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idDrMario"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/idFalco"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idFalco"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idPeach"
        app:layout_constraintTop_toBottomOf="@+id/idEditTextSSB2" />

    <TextView
        android:id="@+id/idMarth"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idMarth"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idBowser"
        app:layout_constraintTop_toBottomOf="@+id/idFalco" />

    <TextView
        android:id="@+id/idYoungLink"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idYoungLink"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idIceClimbers"
        app:layout_constraintTop_toBottomOf="@+id/idMarth" />

    <TextView
        android:id="@+id/idGanondorf"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idGanondorf"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idSheik"
        app:layout_constraintTop_toBottomOf="@+id/idYoungLink" />

    <TextView
        android:id="@+id/idMewtwo"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idMewtwo"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idZelda"
        app:layout_constraintTop_toBottomOf="@+id/idGanondorf" />

    <TextView
        android:id="@+id/idRoy"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idRoy"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idDrMario"
        app:layout_constraintTop_toBottomOf="@+id/idMewtwo" />

    <TextView
        android:id="@+id/idMrGameAndWatch"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginEnd="55dp"
        android:gravity="center"
        android:text="@string/idMrGameAndWatch"
        android:textAlignment="center"
        android:textSize="20sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idPichu"
        app:layout_constraintTop_toBottomOf="@+id/idRoy" />

    <Button
        android:id="@+id/idButtonCheckSSB2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="20dp"
        android:text="@string/idButtonCheckSSB2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/idEditTextSSB2"
        app:layout_constraintTop_toBottomOf="@+id/idScoreSSB2" />
</androidx.constraintlayout.widget.ConstraintLayout>